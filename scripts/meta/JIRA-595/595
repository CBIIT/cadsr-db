select distinct QTL_NAME from  QUEST_CONTENTS_EXT where conte_idseq='6BDC8E1A-E021-BC44-E040-BB89AD4365F6'
and trim(QTL_NAME) ='CRF';

select *from  QUEST_CONTENTS_EXT where trim(QTL_NAME) ='MODULE' and ASL_NAME='RELEASED'
and conte_idseq='6BDC8E1A-E021-BC44-E040-BB89AD4365F6' ;

select FR.QC_ID FORM_ID,FR.LONG_NAME FORM_NAME,CDE_ID,DE.LONG_NAME DE_NAME, MD.QC_ID MODULE_ID,MD.LONG_NAME MODULE_NAME,Q.QC_ID QUESTION_ID,Q.LONG_NAME QUESTION_NAME,Q.DE_IDSEQ,Q.DISPLAY_ORDER
from QUEST_CONTENTS_EXT FR,QUEST_CONTENTS_EXT MD,
QUEST_CONTENTS_EXT Q,DATA_ELEMENTS DE
 where MD.DN_CRF_IDSEQ=FR.QC_IDSEQ
 and trim(FR.QTL_NAME) ='CRF' and FR.ASL_NAME='RELEASED'
and FR.conte_idseq='6BDC8E1A-E021-BC44-E040-BB89AD4365F6' 
and Q.P_MOD_IDSEQ=MD.QC_IDSEQ
and trim( MD.QTL_NAME) ='MODULE'
and trim( Q.QTL_NAME) ='QUESTION'
and MD.long_NAME LIKE 'Mandatory%'
and Q.DE_IDSEQ=DE.DE_IDSEQ
--and MD.DE_IDSEQ is not null
order by FR.QC_ID,MD.QC_ID,Q.DISPLAY_ORDER--DE_IDSEQ;

select FR.QC_ID FORM_ID,FR.LONG_NAME FORM_NAME,CDE_ID,DE.LONG_NAME DE_NAME, MD.QC_ID MODULE_ID,MD.LONG_NAME MODULE_NAME,Q.QC_ID QUESTION_ID,Q.LONG_NAME QUESTION_NAME,Q.DE_IDSEQ,Q.DISPLAY_ORDER
from QUEST_CONTENTS_EXT FR,QUEST_CONTENTS_EXT MD,
QUEST_CONTENTS_EXT Q,DATA_ELEMENTS DE
 where MD.DN_CRF_IDSEQ=FR.QC_IDSEQ
 and trim(FR.QTL_NAME) ='CRF' and FR.ASL_NAME='RELEASED'
and FR.conte_idseq='6BDC8E1A-E021-BC44-E040-BB89AD4365F6' 
and Q.P_MOD_IDSEQ=MD.QC_IDSEQ
and trim( MD.QTL_NAME) ='MODULE'
and trim( Q.QTL_NAME) ='QUESTION'
and MD.long_NAME LIKE 'Optional%'
and Q.DE_IDSEQ=DE.DE_IDSEQ
--and MD.DE_IDSEQ is not null
order by FR.QC_ID,MD.QC_ID,Q.DISPLAY_ORDER--DE_IDSEQ;

select FR.QC_ID FORM_ID,FR.LONG_NAME FORM_NAME,CDE_ID,DE.LONG_NAME DE_NAME, MD.QC_ID MODULE_ID,MD.LONG_NAME MODULE_NAME,Q.QC_ID QUESTION_ID,Q.LONG_NAME QUESTION_NAME,Q.DE_IDSEQ,Q.DISPLAY_ORDER
from QUEST_CONTENTS_EXT FR,QUEST_CONTENTS_EXT MD,
QUEST_CONTENTS_EXT Q,DATA_ELEMENTS DE
 where MD.DN_CRF_IDSEQ=FR.QC_IDSEQ
 and trim(FR.QTL_NAME) ='CRF' and FR.ASL_NAME='RELEASED'
and FR.conte_idseq='6BDC8E1A-E021-BC44-E040-BB89AD4365F6' 
and Q.P_MOD_IDSEQ=MD.QC_IDSEQ
and trim( MD.QTL_NAME) ='MODULE'
and trim( Q.QTL_NAME) ='QUESTION'
and MD.long_NAME LIKE 'Conditional%'
and Q.DE_IDSEQ=DE.DE_IDSEQ
--and MD.DE_IDSEQ is not null
order by FR.QC_ID,MD.QC_ID,Q.DISPLAY_ORDER--DE_IDSEQ;
